# These are loaded from root .env in development
# For Netlify deployment, set these in the Netlify dashboard:

# WalletConnect - get from https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Alchemy RPC URLs
NEXT_PUBLIC_MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_KEY

# Token ID minted by the extension (defaults to 2 if not set)
NEXT_PUBLIC_TOKEN_ID=2
